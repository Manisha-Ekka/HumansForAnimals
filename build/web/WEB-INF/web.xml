<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Registration</servlet-name>
        <servlet-class>com.register.Registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Login</servlet-name>
        <servlet-class>com.register.Login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostReview</servlet-name>
        <servlet-class>com.register.PostReview</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PostIssue</servlet-name>
        <servlet-class>com.register.PostIssue</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Membership</servlet-name>
        <servlet-class>com.register.Membership</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddEvent</servlet-name>
        <servlet-class>com.register.AddEvent</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Registration</servlet-name>
        <url-pattern>/Registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Login</servlet-name>
        <url-pattern>/Login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostReview</servlet-name>
        <url-pattern>/PostReview</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PostIssue</servlet-name>
        <url-pattern>/PostIssue</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Membership</servlet-name>
        <url-pattern>/Membership</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddEvent</servlet-name>
        <url-pattern>/AddEvent</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
